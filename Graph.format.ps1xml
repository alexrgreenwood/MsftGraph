              <ListEntry>
                <ListItems>
                  <ListItem>
                   <label>Subject</label>
                    <PropertyName>subject</PropertyName>
                  </ListItem>
                  <ListItem>
                   <label>All Day</label>
                    <PropertyName>isallday</PropertyName>
