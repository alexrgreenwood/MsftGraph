                            </TableColumnItem>
                            <TableColumnItem>
                                <ScriptBlock>$_.from.emailAddress.name</ScriptBlock>
                                <Alignment>Left</Alignment>
                            </TableColumnItem>
                            <TableColumnItem>
                                <ScriptBlock>$_.categories -join ', '</ScriptBlock>
                                <Alignment>Left</Alignment>
