                      <Label>Topic</Label>
                      <Alignment>left</Alignment>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Received</Label>
                        <Alignment>Left</Alignment>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Sender</Label>
                        <Alignment>Left</Alignment>
                    </TableColumnHeader>
